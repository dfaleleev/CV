{
  "name": "cv",
  "version": "1.0.0",
  "description": "Resume project in JSON resume format",
  "main": "resume.json",
  "scripts": {
    "html": "node ./node_modules/resume-cli/index.js export ./export/resume.html",
    "export": "node ./node_modules/resume-cli/index.js export ./export/resume.pdf",
    "export:md": "node ./node_modules/resume-cli/index.js export --theme md ./export/resume-md.html",
    "export:detail": "node ./node_modules/resume-cli/index.js export ./export/resume-detail.pdf"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dfaleleev/CV.git"
  },
  "keywords": [
    "resume"
  ],
  "author": "Dmitry Faleleev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dfaleleev/CV/issues"
  },
  "homepage": "https://github.com/dfaleleev/CV#readme",
  "dependencies": {
    "jsonresume-theme-md": "^1.1.1",
    "resume-cli": "^1.2.7"
  }
}
